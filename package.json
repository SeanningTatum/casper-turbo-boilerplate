{
  "name": "casper-turbo-boilerplate",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "next.js",
    "tRPC",
    "shadcn",
    "tailwindcss",
    "prisma",
    "storybook",
    "next-auth"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "design-system:add": "pnpm --filter design-system design-system:add",
    "database:generate": "pnpm --filter database generate"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "config": "workspace:*",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0"
}
